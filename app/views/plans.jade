extends layout
block content
        .about
            .container
                h1.m_2 3 Columns
                .plans-view
                    .col-md-4
                        .pricing-table-grid
                            .plans_head
                                h3 Personal
                                h4.m_4
                                    small.m_2 $
                                    | 10
                                    small.m_3(small='') /mo
                                p Ut wisi enim ad minim veniam, quis nostrud exerci tation
                            ul
                                li
                                    a(href='#') 20GB Disk Space
                                li
