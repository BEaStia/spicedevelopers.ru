                                li
                                    a(href='#') 100 Domains
                                li
                                    a(href='#') 430 Email Accounts
                                li
                                    a(href='#') Automatic Cloud Backups
                            a.popup-with-zoom-anim.button(href='#small-dialog') Order Now
                            #small-dialog.mfp-hide
                                .pop_up
                                    .payment-online-form-left
                                        form
                                            h4
                                                span.shipping
                                                | Shipping
                                            ul
                                                li
                                                    input.text-box-dark(type='text', value='Frist Name', onfocus="this.value = '';", onblur="if (this.value == '') {this.value = 'Frist Name';}")
                                                li
                                                    input.text-box-dark(type='text', value='Last Name', onfocus="this.value = '';", onblur="if (this.value == '') {this.value = 'Last Name';}")
                                            ul
                                                li
                                                    input.text-box-dark(type='text', value='Email', onfocus="this.value = '';", onblur="if (this.value == '') {this.value = 'Email';}")
                                                li
                                                    input.text-box-dark(type='text', value='Company Name', onfocus="this.value = '';", onblur="if (this.value == '') {this.value = 'Company Name';}")
                                            ul
                                                li
                                                    input.text-box-dark(type='text', value='Phone', onfocus="this.value = '';", onblur="if (this.value == '') {this.value = 'Phone';}")
                                                li
                                                    input.text-box-dark(type='text', value='Address', onfocus="this.value = '';", onblur="if (this.value == '') {this.value = 'Address';}")
                                                .clearfix
                                            .clear
                                            ul.payment-type
                                                h4
                                                    span.payment
                                                    |  Payments
                                                li
                                                    span.col_checkbox
                                                        input#3.css-checkbox1(type='checkbox')
                                                        label.css-label1(for='3', name='demo_lbl_1')
                                                        a.visa(href='#')
                                                li
                                                    span.col_checkbox
                                                        input#4.css-checkbox2(type='checkbox')
                                                        label.css-label2(for='4', name='demo_lbl_2')
                                                        a.paypal(href='#')
                                                .clearfix
                                            ul
                                                li
                                                    input.text-box-dark(type='text', value='Card Number', onfocus="this.value = '';", onblur="if (this.value == '') {this.value = 'Card Number';}")
                                                li
                                                    input.text-box-dark(type='text', value='Name on card', onfocus="this.value = '';", onblur="if (this.value == '') {this.value = 'Name on card';}")
                                                .clearfix
                                            ul
                                                li
                                                    input#datepicker.text-box-light.hasDatepicker(type='text', value='Expiration Date', onfocus="this.value = '';", onblur="if (this.value == '') {this.value = 'Expiration Date';}")
                                                    em.pay-date
                                                li
                                                    input.text-box-dark(type='text', value='Security Code', onfocus="this.value = '';", onblur="if (this.value == '') {this.value = 'Security Code';}")
                                                .clearfix
                                            ul.payment-sendbtns
                                                li
                                                    input(type='reset', value='Cancel')
                                                li
                                                    input(type='submit', value='Process order')
